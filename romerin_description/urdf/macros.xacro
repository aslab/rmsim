<?xml version="1.0"?>
<robot name="centipede" xmlns:xacro="http://www.ros.org/wiki/xacro">
      
	<xacro:macro name="body" params="parent child">
		<joint name="fixed" type="fixed">
  			<origin rpy="0 0 0" xyz="0 0 0.1"/>
    		<parent link="${parent}"/>
    	    <child link="${child}"/>
		</joint>
	</xacro:macro>
	
</robot>